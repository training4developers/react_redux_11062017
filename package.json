{
  "name": "react_redux_11062017",
  "version": "1.0.0",
  "description": "Starter project for React/Redux/GraphQL/Relay",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && concurrently \"npm run rest\" \"npm run client\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --env jest-environment-node-debug",
    "clean": "rimraf coverage server-dist schema.graphql",
    "server-init": "babel server --out-dir server-dist && node ./server-dist/output-schema",
    "server": "concurrently \"babel server --out-dir server-dist --watch\" \"nodemon --watch server-dist ./server-dist/index\"",
    "client": "\"webpack-dev-server\"",
    "rest": "json-server --watch --port 3010 db.json",
    "relay": "relay-compiler --watch --src ./client/js/ --schema ./schema.graphql",
    "relay:once": "relay-compiler --src ./client/js/ --schema ./schema.graphql",
    "graphql-relay": "npm run server-init && npm run relay:once"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/training4developers/react_07122017.git"
  },
  "author": "Eric W. Greene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/training4developers/react_07122017/issues"
  },
  "homepage": "https://github.com/training4developers/react_07122017#readme",
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-relay": "1.4.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "concurrently": "3.5.0",
    "copy-webpack-plugin": "4.2.0",
    "css-loader": "0.28.7",
    "enzyme": "3.1.1",
    "enzyme-adapter-react-16": "1.0.4",
    "esformatter": "0.10.0",
    "esformatter-jsx": "8.0.1",
    "eslint": "4.10.0",
    "eslint-config-defaults": "9.0.0",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "jest-environment-node-debug": "2.0.0",
    "json-server": "0.12.1",
    "mobx-react-devtools": "4.2.15",
    "node-sass": "4.6.0",
    "nodemon": "1.12.1",
    "path-to-regexp": "2.1.0",
    "postcss": "6.0.14",
    "postcss-loader": "2.0.8",
    "react-test-renderer": "16.0.0",
    "relay-compiler": "1.4.1",
    "relay-runtime": "1.4.1",
    "resolve-url-loader": "2.2.0",
    "rimraf": "2.6.2",
    "rxjs": "5.5.2",
    "sass-lint": "1.12.1",
    "style-loader": "0.19.0",
    "uglify-js": "3.1.7",
    "uglifyjs-webpack-plugin": "1.0.1",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-do-expressions": "6.22.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "color-namer": "1.3.0",
    "es6-promise": "4.1.1",
    "exports-loader": "0.6.4",
    "express": "4.16.2",
    "express-graphql": "0.6.11",
    "found": "0.3.5",
    "found-relay": "0.3.0-alpha.9",
    "graphql": "0.11.7",
    "graphql-relay": "0.5.3",
    "immutable": "3.8.2",
    "imports-loader": "0.7.1",
    "key-mirror": "1.0.1",
    "mobx": "3.3.1",
    "mobx-react": "4.3.4",
    "node-fetch": "1.7.3",
    "null-loader": "0.1.1",
    "pluralize": "7.0.0",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-bootstrap": "0.31.5",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-relay": "1.4.1",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-transition-group": "2.2.1",
    "redux": "3.7.2",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "sass-loader": "6.0.6",
    "semantic-ui-css": "2.2.12",
    "semantic-ui-react": "0.76.0",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./config/test-setup.js"
    ],
    "collectCoverageFrom": [
      "client/**/*.js",
      "server/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devServer": {
    "contentBase": "client-dist",
    "historyApiFallback": true,
    "port": 3000,
    "proxy": [
      {
        "context": [
          "/graphql"
        ],
        "target": "http://localhost:3020",
        "changeOrigin": true,
        "secure": false
      },
      {
        "context": [
          "/api"
        ],
        "target": "http://localhost:3010",
        "changeOrigin": true,
        "secure": false,
        "pathRewrite": {
          "^/api": ""
        }
      }
    ]
  }
}
